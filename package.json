{
  "name": "subscription-economy-backend",
  "version": "0.0.1",
  "description": "Handle Subcriptions template for projects",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "migration:deploy": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/config/data-source.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create src/migrations/migration$(node -p \"new Date().toISOString().replace(/[-:.TZ]/g, '')\")"
  },
  "keywords": [
    "Node.JS",
    "Express",
    "TypeORM",
    "Typescript"
  ],
  "author": "Camilo Hernandez <brayan.hernandez1222@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.2",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.25",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
